{
  "define": [
    "fib",
    { "n": "Int" },
    "Int",
    {
      "=": ["y", 0],
      "if": [
        [{ "==": [{ "$": "n" }, 0] }, { "=": ["y", 0] }],
        [{ "==": [{ "$": "n" }, 1] }, { "=": ["y", 1] }],
        [
          true,
          {
            "=": [
              "y",
              {
                "+": [
                  { "fib": [{ "-": [{ "$": "n" }, 1] }] },
                  { "fib": [{ "-": [{ "$": "n" }, 2] }] }
                ]
              }
            ]
          }
        ]
      ],
      "$": "y"
    }
  ],
  "message": ["", "This program computes fib(10) = 55."],
  "fib": [10]
}
