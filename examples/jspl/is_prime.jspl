define(
  is_prime, { n: Int }, Bool,
  {
    if([$n <= 1, ret(false)])
    if([$n <= 3, ret(true)])
    if([{ { $n % 2 } == 0 } or { { $n % 3 } == 0 }, ret(false)])

    i = 5
    while({$i * $i} <= $n, {
      if([{ { $n % $i } == 0 } or { { $n % { $i + 2 } } == 0 }, ret(false)])
      i = { $i + 6 }
    })

    true
  }
)
flag = is_prime(255)
if ([$flag, print("255 is a prime\n")], [true, print("255 is not a prime\n")])
